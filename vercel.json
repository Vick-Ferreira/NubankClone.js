{
  "version": 2,
  "builds": [
    {
      "src": "serverweb/app.js",
      "use": "@now/node"
    },
    {
      "src": "serverApi/db.json",
      "use": "@now/node"
    }
  ],
  "routes": [
    { "src": "/serverweb/(.*)", "dest": "serverweb/$1" },
    { "src": "/serverApi/(.*)", "dest": "serverApi/$1" },
    { "src": "/(.*)", "dest": "serverweb/app.js" }
  ]
}
