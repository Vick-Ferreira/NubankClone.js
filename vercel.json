{
  "builds": [
    { "src": "serverweb/app.js", "use": "@now/node" },
    { "src": "serverApi/db.json", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/serverweb/(.*)", "dest": "serverweb/app.js" },
    { "src": "/serverApi/(.*)", "dest": "serverApi/db.json" },
    { "src": "/(.*)", "dest": "serverweb/app.js" }
  ]
}